<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Scheduler for Birthday-->
        <record id="ir_cron_birthday_alarm_notification" model="ir.cron">
            <field name="name">Birthday Reminder</field>
            <field name="model_id" ref="model_student_details"/>
            <field name="state">code</field>
            <field name="code">model.birthday_reminder()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
        </record>
    </data>
</odoo>